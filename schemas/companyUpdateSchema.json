{
    "$id": "http://example.com/example.json",
    "type": "object",
    "additionalProperties": false,
    "required": [
      "name"
    ],
    "properties": {
      "name": {
        "$id": "/properties/name",
        "type": "string",
        "title": "The Name Schema ",
        "default": "",
        "examples": ["Rithm"]
      },
      "handle": {
        "$id": "/properties/handle",
        "type": "string",
        "title": "The Handle Schema ",
        "default": "",
        "examples": ["rithm"]
      },
      "description": {
        "$id": "/properties/description",
        "type": "string",
        "title": "The description Schema ",
        "default": "",
        "examples": ["A teaching compnay"]
      },
      "num_employees": {
        "$id": "/properties/num_employees",
        "type": "integer",
        "title": "The Number of employeees Schema ",
        "default": "",
        "examples": [40]
      },
      "logo_url": {
        "$id": "/properties/logo_url",
        "type": "string",
        "title": "The Logo Schema ",
        "default": "",
        "examples": [
          "https://ih1.redbubble.net/image.396326354.4958/flat,800x800,075,t.u1.jpg"
        ]
      },
      "_token": {
        "$id": "/properties/token",
        "type": "string",
        "title": "The Token Schema ",
        "default": "",
        "examples": [
          "foo123"
        ]
      }
    }
  }
  